{
  "project_dir_depth": 2,
  "project_name": "eliseadmin",
  "module_name": "fe",
  "install": {
    "commands": []
  },
  "build": {
    "docker_image": {
      "base_image": "harbor.shopeemobile.com/shopee/nodejs-base",
      "dependent_libraries_files": ["package.json", "package-lock.json"],
      "run_commands": [
        "npm install",
        "pip install -U shopee-deploy-common==0.1.99"
      ]
    },
    "commands": ["npm run build"],
    "upload_static": {
      "static_dir": "build",
      "enable_cdn": true
    }
  },
  "deploy": {
    "idcs": {
      "live": {
        "sg": ["sg1"],
        "my": ["sg1"],
        "th": ["sg1"],
        "ph": ["sg1"],
        "vn": ["sg1"],
        "id": ["sg1"],
        "tw": ["sg1"]
      },
      "liveish": {
        "sg": ["sg1"],
        "my": ["sg1"],
        "th": ["sg1"],
        "ph": ["sg1"],
        "vn": ["sg1"],
        "id": ["sg1"],
        "tw": ["sg1"]
      }
    },
    "resources": {
      "live": {
        "cpu": 1,
        "mem": 1024
      }
    },
    "instances": {
      "live": {
        "sg": 1
      }
    }
  }
}
